<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlagView">
        <attr name="countryCode" format="string"/>
        <attr name="flagStyle" format="enum">
            <enum name="none" value="0"/>
            <enum name="rounded" value="1"/>
            <enum name="square" value="2"/>
            <enum name="circle" value="3"/>
            <enum name="waving" value="4"/>
        </attr>
    </declare-styleable>
</resources>